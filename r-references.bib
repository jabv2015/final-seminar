journal = {American Economic Journal. Applied Economics},
keywords = {School Dropouts ; Crime ; Public Schools ; Economi},
number = {1},
pages = {33--57},
publisher = {American Economic Association},
title = {{Birthdays, Schooling, and Crime: Regression-Discontinuity Analysis of School Performance, Delinquency, Dropout, and Crime Initiation}},
volume = {8},
year = {2016}
