year = {2014}
}
@article{Ravallion2005,
abstract = {Crosscountry comparisons of social indicators controlling for income and/or social spending have been widely used to measure and explain social efficiency, analogously to technical efficiency in production. The article argues that these methods are clouded in ambiguities about what is being measured. Standard methods of measuring technical efficiency require assumptions that are unlikely to hold for social indicators. In the context of a simple parametric model of life expectancy, conditions are identified under which there will be a systematic pattern of bias in estimates of efficient public health spending. },
author = {Ravallion, Martin},
doi = {10.1086/425380},
issn = {00130079},
journal = {Economic Development and Cultural Change},
