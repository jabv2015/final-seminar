@article{Jerrim2015,
abstract = {It is widely believed that countries with greater levels of income inequality also have lower levels of intergenerational mobility. This relationship, known as the Great Gatsby Curve (GGC), has been prominently cited by high-ranking public policymakers, bestselling authors, and Nobel Prize–winning academics. Yet, relatively little cross-national work has empirically examined the mechanisms thought to underpin the GGC—particularly with regard to the role of educational attainment. This paper uses the cross-nationally comparable Programme for International Assessment of Adult Competencies (PIAAC) data set to shed new light on this issue. We find that income inequality is associated with several key components of the intergenerational transmission process—including access to higher education, the financial returns on education, and the residual effect of parental education upon labor-market earnings. Thus, consistent with theoretical models, we find that educational attainment is an important driver of the relationship between intergenerational mobility and income inequality. We hence conclude that unequal access to financial resources plays a central role in the intergenerational transmission of advantage.},
annote = {10.1093/sf/sov075},
author = {Jerrim, John and Macmillan, Lindsey},
doi = {10.1093/sf/sov075},
isbn = {0037-7732},
journal = {Social Forces},
number = {2},
pages = {505--533},
title = {{Income Inequality, Intergenerational Mobility, and the Great Gatsby Curve: Is Education the Key?}},
url = {http://dx.doi.org/10.1093/sf/sov075},
volume = {94},
year = {2015}
}
