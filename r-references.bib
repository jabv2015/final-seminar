keywords = {C26 ; D74 ; H70 ; I3 ; O54 ; Income Inequality ; C},
number = {C},
pages = {128--143},
publisher = {Elsevier B.V.},
title = {{Income inequality and violent crime: Evidence from Mexico's drug war}},
volume = {120},
year = {2016}
