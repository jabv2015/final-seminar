author = {Kao, Chiang.},
doi = {10.1007/978-3-319-31718-2},
isbn = {3-319-31718-0},
keywords = {Business,Business and Management,Decision making,Electronic books,Management science,Operation Research/Decision Theory,Operations Management,Operations Research, Management Science,Operations research,Production management},
publisher = {Springer International Publishing},
