keywords = {DEA operational research,business failure,forecast,fracaso empresarial,investigaci{\'{o}}n operativa DEA,pron{\'{o}}stico},
number = {2},
pages = {65--96},
title = {{DEA as a business failure prediction tool Application to the case of galician SMEs}},
url = {http://www.sciencedirect.com/science/article/pii/S0186104214712550},
volume = {59},
year = {2014}
}
@article{Monkam2014,
abstract = {This paper assesses the technical efficiency of 231 local municipalities in South Africa for 2007 and investigates the potential determinants of efficiency gaps using the non-parametric data envelopment analysis technique. Efficiency scores are explained in a second-stage regression model using a Tobit regression model. To the best of our knowledge, this is the first attempt, using such a technique, to assess technical efficiency at the local government level in the African context. The results show that, on average, B1 and B3 municipalities could have theoretically achieved the same level of basic services with about 16{\%} and 80{\%} fewer resources respectively. Furthermore, fiscal autonomy and the number and skill levels of the top management of a municipality's administration were found to influence the productive efficiency of municipalities in South Africa. Perhaps most importantly, the results depict a bleak picture of the democratic behaviour at the local level in South Africa. It appears that higher income and highly educated households do not feel the incentive to be active participants in public decision-making processes. The paper findings raise concerns over the future of local municipalities in the country, especially about their capability to efficiently deliver expected outcomes on a sustainable basis. },
author = {Monkam, Nara F},
doi = {10.1080/0376835X.2013.875888},
issn = {0376-835X},
journal = {Development Southern Africa},
keywords = {Municipalities ; Spending Efficiency ; Sub-Nationa},
number = {2},
pages = {1--24},
publisher = {Routledge},
title = {{Local municipality productive efficiency and its determinants in South Africa}},
volume = {31},
year = {2014}
}
