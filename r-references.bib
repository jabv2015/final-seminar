title = {{AUSTRALIAN LOCAL GOVERNMENT AMALGAMATION: A CONCEPTUAL ANALYSIS POPULATION SIZE AND SCALE ECONOMIES IN MUNICIPAL SERVICE PROVISION1}},
volume = {14},
year = {2008}
}
