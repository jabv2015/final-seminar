}
@article{Falk2002,
abstract = {According to the standard economic perspective, criminal activities vary with the `price' of crime, which depends on the severity of punishment and the detection probability. An alternative view claims that crime is also influenced by social interaction phenomena, such as `peer pressure' or `neighbourhood effects'. Detecting social interaction in the field is fraught with difficulties, however. In this paper, it is argued that given its extensive control possibilities, experiments are well suited to show the potential existence of social interaction effects. An experimental study where people can engage in `criminal' activities is presented. Support is found for the importance of social interaction: On average subjects steal the more, the more others steal. This behaviour can be interpreted in terms of reciprocity. },
address = {Amsterdam},
author = {Falk, Armin and Fischbacher, Urs},
